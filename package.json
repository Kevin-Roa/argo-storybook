{
	"name": "utd-argo-ux-master-library",
	"version": "0.1.0",
	"private": false,
	"dependencies": {
		"@emotion/react": "^11.10.6",
		"@emotion/styled": "^11.10.6",
		"@mui/icons-material": "^5.11.9",
		"@mui/material": "^5.11.10",
		"@types/jest": "^29.4.0",
		"@types/node": "^18.13.0",
		"@types/react": "^18.0.27",
		"@types/react-dom": "^18.0.10",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-scripts": "5.0.1",
		"typescript": "^4.9.5",
		"web-vitals": "^2.1.4"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "cross-env BABEL_ENV=production babel ./src/index.ts -d dist --extensions '.ts,.tsx'",
		"release": "auto shipit --base-branch main",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"storybook": "start-storybook -p 6006 -s public",
		"build-storybook": "build-storybook -s public"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		],
		"overrides": [
			{
				"files": [
					"**/*.stories.*"
				],
				"rules": {
					"import/no-anonymous-default-export": "off"
				}
			}
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@babel/cli": "^7.21.0",
		"@babel/core": "^7.21.0",
		"@storybook/addon-actions": "^6.5.16",
		"@storybook/addon-essentials": "^6.5.16",
		"@storybook/addon-interactions": "^6.5.16",
		"@storybook/addon-links": "^6.5.16",
		"@storybook/builder-webpack5": "^6.5.16",
		"@storybook/manager-webpack5": "^6.5.16",
		"@storybook/node-logger": "^6.5.16",
		"@storybook/preset-create-react-app": "^4.1.2",
		"@storybook/react": "^6.5.16",
		"@storybook/testing-library": "^0.0.13",
		"auto": "^10.42.2",
		"babel-plugin-named-exports-order": "^0.0.2",
		"chromatic": "^6.17.0",
		"cross-env": "^7.0.3",
		"prop-types": "^15.8.1",
		"webpack": "^5.75.0"
	},
	"babel": {
		"presets": [
			[
				"react-app",
				{
					"absoluteRuntime": false
				}
			]
		]
	},
	"description": "ARGO UX Master Component Library",
	"main": "dist/index.js",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/UTD-ARGO/argo-storybook.git"
	},
	"author": "",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/UTD-ARGO/argo-storybook/issues"
	},
	"homepage": "https://github.com/UTD-ARGO/argo-storybook#readme"
}
